{
  "name": "webdriverio-devtools",
  "version": "1.0.0",
  "description": "WebdriverIO with Chrome DevTools Protocol",
  "scripts": {
    "compile": "tsc --incremental",
    "lint": "eslint --ext .ts --ignore-path .gitignore .",
    "test": "wdio ./wdio.conf.js",
    "test:headless": "npm run test -- --headless"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mgrybyk/webdriverio-devtools.git"
  },
  "keywords": [
    "WebdriverIO",
    "devtools",
    "wdio",
    "mocha",
    "typescript",
    "expect-webdriverio"
  ],
  "author": "mykola.grybyk@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mgrybyk/webdriverio-devtools/issues"
  },
  "homepage": "https://github.com/mgrybyk/webdriverio-devtools#readme",
  "husky": {
    "hooks": {
      "pre-commit": "npm-run-all --parallel compile lint"
    }
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "@types/mocha": "^8.0.4",
    "@wdio/allure-reporter": "^6.8.1",
    "@wdio/cli": "^6.8.1",
    "@wdio/local-runner": "^6.8.1",
    "@wdio/mocha-framework": "^6.8.0",
    "@wdio/spec-reporter": "^6.8.1",
    "@wdio/sync": "^6.8.0",
    "devtools": "^6.8.0",
    "expect-webdriverio": "^1.4.1"
  },
  "bundledDependencies": []
}
