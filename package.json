{
  "name": "webdriverio-devtools",
  "version": "1.0.0",
  "description": "WebdriverIO with Chrome DevTools Protocol",
  "scripts": {
    "compile": "tsc --incremental",
    "lint": "tslint --project tsconfig.json --fix",
    "test": "wdio wdio.CHROME.conf.js",
    "test:headless": "CHROME_HEADLESS=true npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mgrybyk/webdriverio-devtools.git"
  },
  "keywords": [
    "webdriverio",
    "devtools",
    "expect-webdriverio"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mgrybyk/webdriverio-devtools/issues"
  },
  "homepage": "https://github.com/mgrybyk/webdriverio-devtools#readme",
  "husky": {
    "hooks": {
      "pre-commit": "npm-run-all --parallel compile lint"
    }
  },
  "devDependencies": {
    "husky": "^4.2.5",
    "npm-run-all": "^4.1.5",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "@types/mocha": "^7.0.2",
    "@wdio/cli": "^6.1.15",
    "@wdio/local-runner": "^6.1.14",
    "@wdio/mocha-framework": "^6.1.14",
    "@wdio/spec-reporter": "^6.1.14",
    "@wdio/sync": "^6.1.14",
    "devtools": "^6.1.14",
    "expect-webdriverio": "^1.1.5"
  },
  "bundledDependencies": []
}
