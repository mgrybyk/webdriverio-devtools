{
  "name": "webdriverio-devtools",
  "version": "1.0.0",
  "description": "WebdriverIO with Chrome DevTools Protocol",
  "scripts": {
    "compile": "tsc --incremental",
    "lint": "tslint --project tsconfig.json --fix",
    "test": "wdio wdio.CHROME.conf.js",
    "test:headless": "CHROME_HEADLESS=true npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mgrybyk/webdriverio-devtools.git"
  },
  "keywords": [
    "webdriverio",
    "devtools",
    "expect-webdriverio"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mgrybyk/webdriverio-devtools/issues"
  },
  "homepage": "https://github.com/mgrybyk/webdriverio-devtools#readme",
  "husky": {
    "hooks": {
      "pre-commit": "npm-run-all --parallel compile lint"
    }
  },
  "devDependencies": {
    "husky": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "ts-node": "^8.5.4",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.1",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.7.3"
  },
  "dependencies": {
    "@types/mocha": "^5.2.7",
    "@wdio/cli": "^5.16.13",
    "@wdio/local-runner": "^5.16.13",
    "@wdio/mocha-framework": "^5.16.13",
    "@wdio/spec-reporter": "^5.16.11",
    "@wdio/sync": "^5.16.14",
    "devtools": "^5.16.13",
    "expect": "^24.9.0",
    "expect-webdriverio": "^0.1.2-beta.6"
  },
  "bundledDependencies": []
}
