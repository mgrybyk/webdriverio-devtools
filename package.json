{
  "name": "webdriverio-devtools",
  "version": "1.0.0",
  "description": "WebdriverIO with Chrome DevTools Protocol",
  "main": "index.js",
  "scripts": {
    "compile": "tsc --incremental",
    "lint": "tslint --project tsconfig.json --fix",
    "test": "CHROME_HEADLESS=true npm run test:chrome",
    "test:chrome": "wdio ./wdio.CHROME.conf.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mgrybyk/webdriverio-devtools.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mgrybyk/webdriverio-devtools/issues"
  },
  "homepage": "https://github.com/mgrybyk/webdriverio-devtools#readme",
  "husky": {
    "hooks": {
      "pre-commit": "npm-run-all --parallel compile lint"
    }
  },
  "devDependencies": {
    "husky": "^3.0.8",
    "npm-run-all": "^4.1.5",
    "tslint": "^5.20.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.6.3"
  },
  "dependencies": {
    "@types/chai": "^4.2.3",
    "@types/mocha": "^5.2.7",
    "@wdio/cli": "^5.14.3",
    "@wdio/local-runner": "^5.14.3",
    "@wdio/mocha-framework": "^5.14.3",
    "@wdio/spec-reporter": "^5.13.2",
    "@wdio/sync": "^5.14.3",
    "chai": "^4.2.0",
    "devtools": "^5.14.3",
    "ts-node": "^8.4.1",
    "tsconfig-paths": "^3.9.0"
